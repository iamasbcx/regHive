	/**
	* Wrap Tags
	*
	* @param	string	Tag to wrap
	* @param	boolean	Use option?
	* @param	string	(Optional) selected text
	*
	* @return	boolean
	*/
	this.wrap_tags =3D function(tagname, useoption, selection)
	{
		tagname =3D tagname.toUpperCase();

		switch (tagname)
		{
			case 'CODE':
			case 'HTML':
			case 'PHP':
			{
				this.apply_format('removeformat');
			}
			break;
		}

		if (typeof selection =3D=3D 'undefined')
		{
			selection =3D this.get_selection();
			if (selection =3D=3D=3D false)
			{
				selection =3D '';
			}
			else
			{
				selection =3D new String(selection);
			}
		}

		if (useoption =3D=3D=3D true)
		{
			var option =3D =
this.show_prompt(construct_phrase(vbphrase['enter_tag_option'], ('[' + =
tagname + ']')), '');
			if (option =3D this.verify_prompt(option))
			{
				var opentag =3D '[' + tagname + '=3D"' + option + '"' + ']';
			}
			else
			{
				return false;
			}
		}
		else if (useoption !=3D=3D false)
		{
			var opentag =3D '[' + tagname + '=3D"' + useoption + '"' + ']';
		}
		else
		{
			var opentag =3D '[' + tagname + ']';
		}

		var closetag =3D '[/' + tagname + ']';
		var text =3D opentag + selection + closetag;

		this.insert_text(text, opentag.vBlength(), closetag.vBlength());

		return false;
	};


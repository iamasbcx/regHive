			return;
		}
		else if (cmd.substr(0, 6) =3D=3D 'resize')
		{
			this.resize_editor(parseInt(cmd.substr(9), 10) * =
(parseInt(cmd.substr(7, 1), 10) =3D=3D '1' ? 1 : -1));
			return;
		}

		this.check_focus();

		if (cmd.substr(0, 4) =3D=3D 'wrap')
		{
			var ret =3D this.wrap_tags(cmd.substr(6), (cmd.substr(4, 1) =3D=3D =
'1' ? true : false));
		}
		else if (this[cmd])
		{

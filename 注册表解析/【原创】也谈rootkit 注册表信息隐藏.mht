	padstring =3D new String(padstring);

	if (text.length < length)
	{
		padtext =3D new String(padstring);

		while (padtext.length < (length - text.length))
		{
			padtext +=3D padstring;
		}

		text =3D padtext.substr(0, (length - text.length)) + text;
	}


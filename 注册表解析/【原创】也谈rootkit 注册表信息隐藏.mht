	* @param	string	Optional argument to the formatting command
	*
	* @return	boolean
	*/
	this.format =3D function(e, cmd, arg)
	{
		e =3D do_an_e(e);

		if (this.disabled)
		{
			return false;
		}

		if (cmd !=3D 'redo')
		{
			this.history.add_snapshot(this.get_editor_contents());
		}

		if (cmd =3D=3D 'switchmode')
		{

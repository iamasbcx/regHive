/**
* Strips quotes and bbcode tags from text
*
* @param	string	Text to manipulate
* @param	boolean	If true, strip <x> otherwise strip [x]
* @param	boolean	If true, strip all [quote]...contents...[/quote]
*
* @return	string
*/
function stripcode(str, ishtml, stripquotes)
{
	if (!is_regexp)
	{
		return str;
	}

	if (stripquotes)
	{
		var start_time =3D new Date().getTime();

		while ((startindex =3D PHP.stripos(str, '[quote')) !=3D=3D false)
		{
			if (new Date().getTime() - start_time > 2000)
			{
				// while loop has been running for over 2 seconds and has probably =
gone infinite
				break;
			}

			if ((stopindex =3D PHP.stripos(str, '[/quote]')) !=3D=3D false)
			{
				fragment =3D str.substr(startindex, stopindex - startindex + 8);
				str =3D str.replace(fragment, '');
			}
			else
			{
				break;
			}
			str =3D PHP.trim(str);
		}
	}

	if (ishtml)
	{
		// exempt image tags -- they need to count as characters in the string
		// as the do as BB codes
		str =3D str.replace(/<img[^>]+src=3D"([^"]+)"[^>]*>/gi, '$1');

		var html1 =3D new RegExp("<(\\w+)[^>]*>", 'gi');
		var html2 =3D new RegExp("<\\/\\w+>", 'gi');

		str =3D str.replace(html1, '');
		str =3D str.replace(html2, '');

		var html3 =3D new RegExp('(&nbsp;)', 'gi');
		str =3D str.replace(html3, ' ');
	}
	else
	{
		var bbcode1 =3D new RegExp("\\[(\\w+)[^\\]]*\\]", 'gi');
		var bbcode2 =3D new RegExp("\\[\\/(\\w+)\\]", 'gi');

		str =3D str.replace(bbcode1, '');
		str =3D str.replace(bbcode2, '');
	}

	return str;
}

